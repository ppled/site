$xs-max: 480px;
$sm-max: 600px;
$md-min: ($sm-max + 1);
$md-max: 768px;
$lg-min: ($md-max + 1);

@keyframes load-pulse {
    0% {
    	transform: translate(-50%, -50%) scale(0);
    }

    100% {
    	transform: translate(-50%, -50%) scale(1);
    	opacity: 0;
    }
}

.load-pulse {
    background-color: #f5bc2a;
    width: 80px;
    height: 80px;
    position: absolute;
    top: 50%;
    left: 50%;
    border-radius: 50%;
    transform: translate(-50%, -50%);
    animation: load-pulse 1.0s infinite ease-in-out;
}

.wrapper.main-content {

    @media screen and (max-width: $sm-max) {
        margin-top: 40px;
    }
}

.flexslider {
    overflow: hidden;

    & + hr {

        @media screen and (max-width: $sm-max) {
            margin-top: 10px;
        }
    }

    .slides img {
        display: none;

        @media screen and (max-width: $sm-max) {

            &.slide-image-sm {
                display: block;
            }
        }

        @media screen and (min-width: $md-min) {

            &.slide-image-lg {
                display: block;
            }
        }
    }

    // loading
    &.loading {

        .height-fill {
            position: relative;

            @media screen and (max-width: $sm-max) {
                padding-bottom: 72.79549719%;
            }

            @media screen and (min-width: $md-min) {
                padding-bottom: 41.2371134%;
            }
        }
    }

    // not loading
    &:not(.loading) {

        .height-fill {
            display: none;
        }
    }
}

.flex-direction-nav a {

    &.flex-prev {
        background-image: url('data:image/png;base64,');
    }

    &.flex-next {
        background-image: url('data:image/png;base64,');
    }
}

.featured-collections .grid__item .grid-link__title {

    @media (max-width: $sm-max) {
        height: 40px;
        max-height: 40px;
        overflow: hidden;
    }

    @media (min-width: $lg-min) {
        height: 40px;
        max-height: 40px;
        overflow: hidden;
    }
}

.grid-link--focus {

    @media screen and (min-width: $md-min) {
        padding: 25px;
    }
}

.grid-link__image {
    background-color: #fff;

    @media screen and (min-width: $md-min) {
        margin: 0 auto 15px;
    }

    img {

      	@media screen and (min-width: $md-min) and (max-width: $md-max) {
          	max-height: 200px;
        }
    }
}

.grid-link__image-centered {

  	@media screen and (min-width: $md-min) and (max-width: $md-max) {
  		  height: 200px;
    }
}

.about-float-img {
    width: 300px;
    padding-right: 20px;
    float: left;

    @media screen and (max-width: $xs-max) {
        width: 100%;
        margin-bottom: 15px;
        padding: 0;
        float: none;
    }
}

// custom product fields
.pae-box {
    display: none;
}

.photo-gallery {

    a {
        color: inherit;
    }

    .grid-link__title {
        font-size: 14px;
        text-align: center;
    }
}
