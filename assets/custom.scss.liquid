.flexslider .slides img {
    display: none;
}

.flex-direction-nav a.flex-prev {
    background-image: url('data:image/png;base64,');
}

.flex-direction-nav a.flex-next {
    background-image: url('data:image/png;base64,');
}

.grid-link__image {
    background-color: #fff;
}

.about-float-img {
    width: 300px;
    padding-right: 20px;
    float: left;
}

.photo-gallery {

    a {
        color: inherit;
    }

    .grid-link__title {
        font-size: 14px;
        text-align: center;
    }
}

@media screen and (min-width: 601px) and (max-width: 768px) {

    .grid-link__image-centered {
        height: 200px;
    }

    .grid-link__image img {
        max-height: 200px;
    }
}

@media screen and (min-width: 601px) {

    .flexslider .slides img.slide-image-lg {
        display: block;
    }

    .grid-link--focus {
        padding: 25px;
    }

    .grid-link__image {
        margin: 0 auto 15px;
    }
}

@media screen and (max-width: 600px) {

    body.template-index .main-content {
        margin-top: 40px;
    }

    .flexslider {

        & + hr {
            margin-top: 10px;
        }

        .slides img.slide-image-sm {
            display: block;
        }
    }
}

@media screen and (max-width: 480px) {
    
    .about-float-img {
        width: 100%;
        margin-bottom: 15px;
        padding: 0;
        float: none;
    }
}